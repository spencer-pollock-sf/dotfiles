#!/bin/bash

mkdir ~/Documents
mkdir ~/Downloads
mkdir ~/Media

os=${OSTYPE//[0-9.-]*/}
case "$os" in
    darwin)
        ./.dotfile_scripts/mac_setup
      ;;
    msys)
        echo "Windows not supported"
      ;;
    linux)
        ./.dotfile_scripts/linux_setup
      ;;
    *)
    echo "Unknown OS $OSTYPE"
    exit 1
esac
